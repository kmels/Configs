#!/bin/bash

#invert Super_L and Alt_L
xmodmap -e 'keycode 133=0xffe9' &
xmodmap -e 'keycode  64=0xffeb' &

#start favorite WM
~/.cabal/bin/xmonad &

#start favorite programs
emacs ~/Dropbox/org/kmels.org &
conkeror "mail.kmels.net" &
clementine &
terminator &
bitlbee -c ~/.bitlbee.conf &
xchat & 
skype &
